{
    "name": "chrome-vue-extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://localhost:5173/",
                "https://uk.indeed.com/"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}